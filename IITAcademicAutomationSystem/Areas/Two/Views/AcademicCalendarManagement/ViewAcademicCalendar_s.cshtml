@{
    ViewBag.Title = "View Academic Calendar";
}
<script src='~/Areas/Two/Library/angular.min.js'></script>

<script src="~/Areas/Two/Scripts/Utility/Module/Utility.module.js" type="text/javascript"></script>
<script src="~/Areas/Two/Scripts/AcademicCalendarManagement/Module/AcademicCalendarManagement.module.js" type="text/javascript"></script>
<script src="~/Areas/Two/Scripts/AcademicCalendarManagement/Controller/ViewOrDownloadAcademicCalendar_s.Controller.js" type="text/javascript"></script>
<script src="~/Areas/Two/Scripts/AcademicCalendarManagement/Factory/AcademicCalendarManagement.Factory.js" type="text/javascript"></script>
<script src="~/Areas/Two/Scripts/Utility/Factory/Utility.Factory.js" type="text/javascript"></script>

<div class="container-fluid">
    <div>
        <div ng-app="academicCalendarManagement_module" ng-controller="viewAcademicCalendar_controller">
            <div ng-cloak>
                <div ng-if="message.content!=''">
                    <div class="{{message.color}}">{{message.content}}</div>
                </div>
                <div>
                    <div>
                        <div ng-if="flag.isAcademicCalendarUploaded==false">
                            <div class="col-sm-12">
                                <div class="col-sm-12">
                                    <div class="alert alert-info">
                                        <h3>Academic Calendar Is Not Uploaded Yet</h3>
                                    </div>
                                </div>
                            </div>
                            
                        </div>
                        <div ng-if="flag.isAcademicCalendarUploaded==true">
                            <div class="row">
                                <div>
                                    <div class="embed-responsive embed-responsive-16by9">
                                        <iframe class="embed-responsive-item" src="{{selected.path}}" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>  
                </div>
            </div>
        </div>
    </div> 
</div>   











